{
	"name": "cms-projeto",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --mode dev --port 3002 --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"build:docker": "docker build --target prod-image -t renangalvao/project-cms:latest .",
		"postbuild:docker": "docker push renangalvao/project-cms:latest"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.5.0",
		"@types/google.maps": "^3.52.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"axios": "^1.3.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.59.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-icons-pack": "^2.1.0",
		"svelte-json-tree": "^1.0.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3",
		"yup": "^1.0.2"
	},
	"type": "module",
	"dependencies": {
		"typesafe-i18n": "^5.24.2"
	}
}
