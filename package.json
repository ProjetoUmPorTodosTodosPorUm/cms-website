{
	"name": "cms",
	"version": "1.1.0",
	"description": "projetoumportodostodosporum.org's cms",
	"author": "https://github.com/RenanGalvao",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev --port 3002 --strictPort --host",
		"build": "vite build",
		"build:docker:preview": "docker build --no-cache --target preview-image -t project/cms:preview .",
		"build:docker": "docker build --target prod-image -t renangalvao/project:cms-$npm_package_version -t renangalvao/project:cms-latest .",
		"postbuild:docker": "docker push --all-tags renangalvao/project",
		"preview": "vite preview --port 3002 --strictPort --host",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@googlemaps/js-api-loader": "^1.15.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-node": "^2.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tinymce/tinymce-svelte": "^3.0.0",
		"@types/google.maps": "^3.52.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"css-tree": "^2.3.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.37.0",
		"ioredis": "^5.4.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"rollup-plugin-copy": "^3.5.0",
		"sass": "^1.59.3",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"svelte-icons-pack": "^3.1.3",
		"svelte-json-tree": "^2.2.0",
		"svelte-kit-connect-redis": "^0.1.0",
		"svelte-kit-sessions": "^0.3.2",
		"tinymce": "^7.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^1.0.0",
		"yup": "^1.4.0"
	},
	"type": "module"
}
